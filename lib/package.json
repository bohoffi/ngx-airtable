{
  "name": "ngx-airtable",
  "description": "An Angular module wrapping the Airtable API",
  "version": "2.0.1",
  "homepage": "https://github.com/bohoffi/ngx-airtable",
  "author": {
    "name": "bohoffi",
    "url": "http://github.com/bohoffi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bohoffi/ngx-airtable.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "ng",
    "ngx",
    "angular",
    "library",
    "airtable",
    "API",
    "wrapper"
  ],
  "bugs": {
    "url": "https://github.com/bohoffi/ngx-airtable/issues"
  },
  "main": "bundles/ngx-airtable.umd.js",
  "typings": "index.d.ts",
  "scripts": {
    "cleanup": "rimraf bundles",
    "ngc": "ngcc",
    "bundling": "rollup -c ../rollup.config.js",
    "minify": "uglifyjs bundles/ngx-airtable.umd.js --screw-ie8 --compress --mangle --comments --output bundles/ngx-airtable.umd.min.js",
    "build": "npm run cleanup && ngc && npm run bundling && npm run minify",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "~10.1.2",
    "@angular/common": "~10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/language-service": "~10.1.2",
    "@types/core-js": "^0.9.41",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rxjs": "~6.6.3",
    "ts-node": "~7.0.0",
    "tslint": "^6.0.0",
    "typescript": "~4.0.3",
    "uglify-js": "^3.0.10",
    "zone.js": "~0.10.2"
  },
  "peerDependencies": {
    "@angular/common": ">=8.0.0",
    "@angular/core": ">=8.0.0"
  }
}
